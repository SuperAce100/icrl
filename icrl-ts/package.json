{
  "name": "icrl",
  "version": "0.1.0",
  "description": "In-Context Reinforcement Learning for LLM Agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "example:basic": "tsx examples/basic.ts",
    "example:batch": "tsx examples/batch.ts",
    "example:database": "tsx examples/database.ts",
    "example:retriever": "tsx examples/retriever.ts",
    "example:loop": "tsx examples/loop.ts",
    "example:curation": "tsx examples/curation.ts",
    "example:models": "tsx examples/models.ts",
    "example:providers": "tsx examples/providers.ts",
    "examples:run": "bun run example:basic && bun run example:batch && bun run example:database && bun run example:retriever && bun run example:loop && bun run example:curation && bun run example:models && bun run example:providers",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "icrl",
    "in-context-learning",
    "reinforcement-learning",
    "llm",
    "agents",
    "ai",
    "machine-learning",
    "rag",
    "retrieval"
  ],
  "author": "Asanshay Gupta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperAce100/icrl.git"
  },
  "dependencies": {
    "zod": "^3.23.0",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "openai": "^4.0.0",
    "@anthropic-ai/sdk": "^0.20.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "tsx": "^4.20.3",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.20.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
