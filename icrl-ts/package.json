{
  "name": "icrl",
  "version": "0.1.0",
  "description": "In-Context Reinforcement Learning for LLM Agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "npm run tests:run",
    "test:run": "npm run tests:run",
    "test:vitest": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "release:check": "npm run build && npm run typecheck && npm run tests:run && npm pack --dry-run",
    "test:basic": "tsx tests/basic.ts",
    "test:batch": "tsx tests/batch.ts",
    "test:database": "tsx tests/database.ts",
    "test:retriever": "tsx tests/retriever.ts",
    "test:loop": "tsx tests/loop.ts",
    "test:curation": "tsx tests/curation.ts",
    "test:models": "tsx tests/models.ts",
    "test:providers": "tsx tests/providers.ts",
    "tests:run": "npm run test:basic && npm run test:batch && npm run test:database && npm run test:retriever && npm run test:loop && npm run test:curation && npm run test:models && npm run test:providers",
    "example:openai": "tsx examples/openai-demo.ts",
    "example:anthropic": "tsx examples/anthropic-demo.ts",
    "example:support": "tsx examples/support-triage-demo.ts",
    "example:incident": "tsx examples/incident-response-demo.ts",
    "example:web": "tsx examples/web-convex-demo.ts",
    "example:live:openai": "npm run example:openai",
    "example:live:anthropic": "npm run example:anthropic",
    "examples:run": "npm run example:openai && npm run example:anthropic && npm run example:support && npm run example:incident",
    "examples:run:all": "npm run examples:run && npm run example:web",
    "examples:run:live": "npm run examples:run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "icrl",
    "in-context-learning",
    "reinforcement-learning",
    "llm",
    "agents",
    "ai",
    "machine-learning",
    "rag",
    "retrieval"
  ],
  "author": "Asanshay Gupta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperAce100/icrl.git",
    "directory": "icrl-ts"
  },
  "homepage": "https://icrl.dev",
  "bugs": {
    "url": "https://github.com/SuperAce100/icrl/issues"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "zod": "^3.23.0",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "openai": "^4.0.0",
    "@anthropic-ai/sdk": "^0.20.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "tsx": "^4.20.3",
    "convex": "^1.31.6",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.20.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
